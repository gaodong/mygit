集合类型
	数组
		初始化
		var array = Int[]()
		var array:Int[] = []
		var array:Double[] = Double[](count:3, repeatedValue:0.0)
		var array = Array(count:3, repeatedValue:0)

		var shoppingList:String[] = []
		var shoppingList = []
		var shoppingList = ["eggs", "milk"]
	数组数量
		shoppingList.count
	是否为空
		shoppingList.count == 0 
		shoppingList.isEmpty
	增加数组
		shoppingList += "ss"
		shoppingList.append("cc")
	取值
		shoppingList[0]
	赋值
		shoppingList[0] = "ccdd"
		shoppingList[0...3] = ["cc", "dd"]
	插入值
		shoppingList.insert("ddd", atIndex:0)
	删除值	
		var oldVal = shoppingList.removeAtIndex(0)
		var oldval = shoppingList.removeLast()
	数组的遍历
		for _ in shoppingList {
			...
		}

		for (index, val) in enumerate(shoppingList) {
			... of index is val
		}


	字典
		key - value

		定义
			var dic = Dictionary<String, String>()
			var dic

			var dic = [:]
			var dic = ["name":"gordon", "age":"23"]
			var dic:Dictionary<String, String> = ["name":"gordon", "age":"23"]

		个数
			dic.count
		取值
			var value:String? = dic["name"]
		赋值
			dic["name"] = "haha"

			var oldValue:String? = dic.updateValue("xixi", forKey:"name")	
		删除
			var oldValue:String? = dic.removeValueForKey("xixi")	
		遍历
			for (key, val) for dic {
				...
			}

			for key in dic.keys {

			}

			for val in dic.values {
				...
			}
			var array = Array(dic.values)

